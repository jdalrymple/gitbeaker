{
  "preset": "ts-jest",

  "testEnvironment": "node",
  "testRegex": "test\\/.*\\.ts$",
  "testMatch": null,
  "testURL": "http://localhost/",

  "coverageDirectory": "./coverage/",
  "collectCoverage": true,

  "setupFilesAfterEnv": ["jest-extended"],

  "globals": {
    "ts-jest": {
      "diagnostics": {
        "ignoreCodes": [2322, 2445, 7006, 7031, 2339, 7016, 7023, 7017, 2345, 6133]
      }
    }
  }
}
